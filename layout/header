<script>
    $("#main-menu .dt-menu-toggle i.fa-bars").click(function() {
		$("#main-menu #menu-main-menu").attr("style", "clip-path: circle(100%);");
        $("#main-menu .dt-menu-toggle i.fa-bars").attr("style", "display:none;");
        $("#main-menu .dt-menu-toggle i.fa-times").attr("style", "display:block;");
	});

    $("#main-menu .dt-menu-toggle i.fa-times").click(function() {
		$("#main-menu #menu-main-menu").attr("style", "");
        $("#main-menu .dt-menu-toggle i.fa-bars").attr("style", "display:block;");
        $("#main-menu .dt-menu-toggle i.fa-times").attr("style", "display:none;");
	});

    $(".search-box span").click(function() {
        var textSearch = $(".search-box input").val();
        textSearch = textSearch.toLowerCase();
        textSearch = textSearch.replace(/ /g, "-");
        textSearch = removeAccents(textSearch);
        var url = "./search.html?search=" + textSearch;
        window.location.href = url;
    });

    $(".search-box-2 span").click(function() {
        var textSearch = $(".search-box-2 input").val();
        textSearch = textSearch.toLowerCase();
        textSearch = textSearch.replace(/ /g, "-");
        textSearch = removeAccents(textSearch);
        var url = "./search.html?search=" + textSearch;
        window.location.href = url;
    });

    function removeAccents(str) {
        var AccentsMap = [
            "aàảãáạăằẳẵắặâầẩẫấậ",
            "dđ",
            "eèẻẽéẹêềểễếệ",
            "iìỉĩíị",
            "oòỏõóọôồổỗốộơờởỡớợ",
            "uùủũúụưừửữứự",
            "yỳỷỹýỵ"
        ];
        for (var i = 0; i < AccentsMap.length; i++) {
            var re = new RegExp('[' + AccentsMap[i].substr(1) + ']', 'g');
            var char = AccentsMap[i][0];
            str = str.replace(re, char);
        }
        return str;
    }
</script>